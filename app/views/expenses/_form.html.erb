<%= form_tag("/expenses", method: "post") do %>
  <%= label_tag("$ Housing") %>
  <%= text_field_tag(:Housing_amount) %>
  <%= label_tag("$ Transportation") %>
  <%= text_field_tag(:Transportation_amount) %>
  <%= label_tag("$ Food") %>
  <%= text_field_tag(:food_amount) %>
  <%= label_tag("$ Phone") %>
  <%= text_field_tag(:phone_amount) %>
  <%= label_tag("$ Misc.") %>
  <%= text_field_tag(:misc_amount) %>
  <%= button_tag 'Add Expenses', html: {:class => "button round"} %>
<% end %>

